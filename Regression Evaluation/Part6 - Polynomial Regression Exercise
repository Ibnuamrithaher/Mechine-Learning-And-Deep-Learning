        "## **Silahkan dicoba**"
      ],
      "metadata": {
        "id": "iwgVhtFHgdst"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "preprocessor = ColumnTransformer([\n",
        "    (\"numeric\", num_pipe(poly=2, transform='yeo-johnson'), ['passenger_count','year','distanc']),\n",
        "    (\"categoric\", cat_pipe(encoder='onehot'), [\"month\",\"day\",\"hour\"]),\n",
        "])\n",
        "\n",
        "pipeline = Pipeline([\n",
        "    (\"prep\",preprocessor),\n",
        "    (\"algo\",LinearRegression())\n",
        "])\n",
        "\n",
